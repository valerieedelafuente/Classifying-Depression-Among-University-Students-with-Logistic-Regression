depression_data$gender <- factor(depression_data$gender)
# Fixing the `city` variable to change invalid entries
depression_data <- depression_data %>%
mutate(city = case_when(
city == "Khaziabad" ~ "Ghaziabad",
city == "Nalyan" ~ "Kalyan",
